<head>
<link rel="stylesheet" type="text/css" href="/css/create.css">
</head>

<body>
<form method="post" action="/create_contact">
    <input type="text" placeholder="First Name" name="firstname" maxlength="10" required><br>
    <input type="text" placeholder="Last Name" name="lastname" maxlength="10" required><br>
    <input type="text" placeholder="Street" name="street" maxlength="10" required><br>
    <input type="text" placeholder="City" name="city" maxlength="10" required><br>
    <input type="text" placeholder="State" name="state" maxlength="10" required><br>
    <input type="number" placeholder="Zip" name="zip" min="5" max="5" required><br>
    <input type="number" placeholder="Phone Number" name="phonenumber" min="10" max="10" required><br>

    <input type='submit'>
</form>
</body>
